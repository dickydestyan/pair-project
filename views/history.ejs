<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
    crossorigin="anonymous"></script>
  <title>History</title>
</head>
<!-- Navbar content -->
<%- include('_navbar') %>

<form action="/history" method="get" role="search" style="gap: 20px; margin-bottom: 50px; margin-top: 30px;">
  <input name="search" type="text" placeholder="Search by user name" class="input input-bordered w-24 md:w-auto"
      style="width: 40%;" />
  <button class="btn btn-outline-success btn-primary" type="submit">Search</button>
</form>

  <!-- Table history -->
  <div class="overflow-x-auto">
    <table class="table">
      <!-- head -->
      <thead>
        <tr>
          <th>No.</th>
          <th>Code Transition</th>
          <th>User Name</th>
          <th>Seat Number</th>
          <th>Airlines Name</th>
          <th>Action</th>
        </tr>
      </thead>
      <tbody>
        <!-- row 1 -->
         <% data.forEach((el, idx) => { %>
          <tr class="bg-base-200">
            <th><%= idx+1 %></th>
            <td><%= el.codeTrx %></td>
            <td><%= el.User.name %></td>
            <td><%= el.Ticket.seatNumber %></td>
            <td><%= el.Ticket.Airline.name %></td>
            <td>
              <a href="/invoice/<%= el.id %>" class="btn btn-primary">See Invoice</a>
            </td>
          </tr>
          
         <% }) %>
      </tbody>
    </table>
  </div>















